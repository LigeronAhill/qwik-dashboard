{
    "name": "my-qwik-empty-starter",
    "description": "Blank project with routing included",
    "engines": {
        "node": "^18.17.0 || ^20.3.0 || >=21.0.0"
    },
    "engines-annotation": "Mostly required by sharp which needs a Node-API v9 compatible runtime",
    "private": true,
    "type": "module",
    "scripts": {
        "build": "qwik build",
        "build.client": "vite build",
        "build.preview": "vite build --ssr src/entry.preview.tsx",
        "build.server": "qwik check-client src dist && vite build -c adapters/vercel-edge/vite.config.ts",
        "build.types": "tsc --incremental --noEmit",
        "deploy": "vercel deploy",
        "dev": "vite --mode ssr",
        "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
        "drizzle:check": "drizzle-kit check",
        "drizzle:generate": "drizzle-kit generate",
        "drizzle:migrate": "drizzle-kit migrate",
        "drizzle:pull": "drizzle-kit pull",
        "drizzle:push": "drizzle-kit push",
        "drizzle:studio": "drizzle-kit studio",
        "drizzle:up": "drizzle-kit up",
        "fmt": "npx @biomejs/biome check --write",
        "fmt.check": "npx @biomejs/biome check",
        "lint": "eslint \"src/**/*.ts*\"",
        "postinstall": "drizzle-kit generate",
        "preview": "qwik build preview && vite preview --open",
        "start": "vite --open --mode ssr",
        "qwik": "qwik",
        "seed": "tsx ./script/seed.ts"
    },
    "devDependencies": {
        "@biomejs/biome": "2.3.11",
        "@builder.io/qwik": "^1.18.0",
        "@builder.io/qwik-city": "^1.18.0",
        "@eslint/js": "latest",
        "@qwikest/icons": "^0.0.13",
        "@tailwindcss/vite": "^4.1.4",
        "@types/bcrypt": "^6.0.0",
        "@types/node": "20.19.0",
        "@types/pg": "^8.16.0",
        "bcrypt": "^6.0.0",
        "drizzle-kit": "^0.30.6",
        "drizzle-orm": "^0.39.3",
        "eslint": "9.32.0",
        "eslint-plugin-qwik": "^1.18.0",
        "globals": "16.4.0",
        "pg": "^8.16.3",
        "prettier": "3.6.2",
        "prettier-plugin-tailwindcss": "^0.6.11",
        "tailwindcss": "^4.1.4",
        "tsx": "^4.21.0",
        "typescript": "5.4.5",
        "typescript-eslint": "8.38.0",
        "typescript-plugin-css-modules": "latest",
        "undici": "*",
        "vercel": "^29.1.1",
        "vite": "7.2.6",
        "vite-tsconfig-paths": "^4.2.1"
    },
    "dependencies": {
        "@fontsource-variable/inter": "^5.2.8",
        "@neondatabase/serverless": "^1.0.2",
        "dotenv": "^17.2.3"
    }
}
